  "resolutions": {
    "graphql": "16.6.0"
  },
  "devDependencies": {
    "@example/eslint-config-custom": "0.0.1",
    "@swc/cli": "0.1.59",
    "@swc/core": "1.3.29",
    "@types/node": "18.11.18",
    "eslint": "8.32.0",
    "prettier": "2.8.3",
    "turbo": "1.7.0",
    "typescript": "4.9.4"
  }
}
