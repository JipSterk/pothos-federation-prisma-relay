  "scripts": {
    "prettier": "prettier --write './{apps,packages}/**/src/**/*.{ts,tsx}'",
    "prettier:check": "prettier --check './{apps,packages}/**/src/**/*.{ts,tsx}'",
    "start:services": "turbo run dev --filter=@example/*-service",
    "start:gateway": "turbo run dev --filter=@example/gateway",
